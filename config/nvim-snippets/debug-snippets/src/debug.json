{
  "background color of component": {
    "prefix": "djbg",
    "body": [
      "{backgroundColor:'$0'}"
    ],
    "description": "set background color"
  },
  "print log": {
    "prefix": "djmsg",
    "body": [
      "// import Log from '@libs/Log';",
      "Log.info('')",
