[core]
	excludesfile = ~/.gitignore_global
	compression = 0
    packedGitLimit = 512m
    packedGitWindowSize = 512m
	autocrlf = input
 [pack]
    deltaCacheSize = 2047m
    packSizeLimit = 2047m
    windowMemory = 2047m
[http]
    version = HTTP/1.1
    sslVerify = false
	postBuffer = 524288000
[https]
    sslVerify = false
	postBuffer = 524288000
[protocol "file"]
	allow = always
